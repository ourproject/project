    <%= f.label :number_of_questions %><br>
    <%= f.number_field :number_of_questions %>
  </div>
  <div class="field">
    <%= f.label :duration %><br>
    <%= f.number_field :duration %>
  </div>
  <div class="field">
    <%= f.label :cutoff %><br>
    <%= f.number_field :cutoff %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
  <div>
  <%= button_to 'Logout', logout_path, :method =>"delete" %>
</div>
<% end %>
